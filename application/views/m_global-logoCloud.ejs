<!--  Module - Logo Cloud - "Shows a Cloud of Logos associated to the Brand, Attribute, Industry, Color"  -->
     <div class="container-fluid" id="logocloud">
      <div class="span12 well">
        <div class="title" >
          <button type="button" class="btn btn-default">      
            Industry Logo Cloud
          </button>
        </div>
        <div class="well-content">
         <div class="logoCloudFilter">
            <a href="#" data-filter=".firstdecile">1 - 10</a>
            <a href="#" data-filter=".seconddecile">11 - 20</a>
            <a href="#" data-filter=".thirddecile">21 - 30</a>
            <a href="#" data-filter=".fourthdecile">31 - 40</a>
            <a href="#" data-filter=".fifthdecile">41 - 50</a>
        </div>
          <div class="logoCloudContainer">
            <% for(var i=0; i<industryResult.length; i++){ 
                  var className = '';                  
                  if(i<10){
                    className = 'firstdecile';
                  } 
                  else if(i>=10 && i<20){
                    className = 'seconddecile';
                  }
                  else if(i>=20 && i<30){
                    className = 'thirddecile';
                  }
                  else if(i>=30 && i<40){
                    className = 'fourthdecile';
                  }
                  else{
                    className = 'fifthdecile';
                  }

            %>
                <div class='first <%= className %>'>
                  <%if(typeof industryResult[i].logoFileName !== 'undefined'){%>
                    <img class='logo' src="../logos/<%=industryResult[i].logoFileName%>" >
                  <%}%>
                  </p>
                </div>
              <%}%>
            </div>
          </div>
        </div>
      </div>


