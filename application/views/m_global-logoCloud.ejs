<!--  Module - Logo Cloud - "Shows a Cloud of Logos associated to the Brand, Attribute, Industry, Color"  -->
     <div class="container-fluid" id="logocloud" style="width:47%;margin-right:2%;float:left;">
      <div class="span12 well">
        <div class="title" >
          <button type="button" class="btn btn-default">      
            Industry Logo Cloud
          </button>
            <span class="pull-right muted">
              <div class="tooltips">
              </div>
            </span>
        </div>
        <div class="well-content" style="height:460px;margin-bottom: -20px; ">
         <div class="logoCloudFilter" style="margin-top: 20px;">
            <a href="#" data-filter=".firstdecile"  style="margin-left: 20px;">1 - 20</a>
            <a href="#" data-filter=".secondquartile">21 - 40</a>
            <a href="#" data-filter=".thirdquartile">41 - 60</a>
            <a href="#" data-filter=".fourthquartile">61 - 80</a>
            <a href="#" data-filter=".fourthquartile">81 - 100</a>
        </div>
          <div class="logoCloudContainer">
            <% for(var i=0; i<industryResult.length; i++){ %>
              <% if(industryResult[i].shortName!==brandResult.shortName){ %>
                <div class='first' >
                  <%if(typeof industryResult[i].logoFileName !== 'undefined'){%>
                    <img class='logo' src="../Logos/<%=industryResult[i].logoFileName%>" width='250px'>
                  <%}%>
                  </p>
                </div>
              <%}}%>
            </div>
          </div>
        </div>
      </div>


