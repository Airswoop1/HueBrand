	<script>


 /**
 	used for the color-map in color.html
 **/
	topCountries = [];
	theQueriedColor = {};

	<% for(var k=0; k < topCountries.length ;k++){ %>
		topCountries.push({key:'<%=topCountries[k].key%>', "freq": '<%=topCountries[k].freq %>', 'city': '<%=topCountries[k].city %>'})
	<%}%>

	theQueriedColor = {
		RrgbValue : <%= colorResult.RrgbValue%>,
		GrgbValue : <%= colorResult.GrgbValue%>,
		BrgbValue : <%= colorResult.BrgbValue%>,
	}

	/*******
	used for the color frequency chart in brand.html
	*******/
	topColor = []

	<% for(var l=0; l<topColors.length; l++){%>

		var hexValue = '#' + componentToHex(<%=topColors[l].RrgbValue%>) + componentToHex(<%=topColors[l].GrgbValue%>) + componentToHex(<%=topColors[l].BrgbValue%>);
		
		topColor.push({
			RrgbValue : <%=topColors[l].RrgbValue%>,
			GrgbValue : <%=topColors[l].GrgbValue%>,
			BrgbValue : <%=topColors[l].BrgbValue%>,
			colorPercentage : <%=topColors[l].colorPercentage%>,
			colorName : "<%=topColors[l].colorName%>",
			colorFamily : "<%=topColors[l].colorFamily%>",
			"hexValue" : hexValue
		})
	<%}%>

	function componentToHex(c){
    var hex = c.toString(16);
    return hex.length == 1 ? "0" + hex : hex;
  }

  	topColorsForIndustry = []

	<% 
	for(var m=0; m<topColorsForIndustry.length; m++){%>
		var hexValue = '#' + componentToHex(<%=topColorsForIndustry[m].RrgbValue%>) + componentToHex(<%=topColorsForIndustry[m].GrgbValue%>) + componentToHex(<%=topColorsForIndustry[m].BrgbValue%>);

		topColorsForIndustry.push({
			colorPercentage : <%=topColorsForIndustry[m].colorPercentage%>,
			colorFamily : "<%=topColorsForIndustry[m].colorFamily%>",
			"hexValue" : hexValue,
			RrgbValue : <%=topColorsForIndustry[m].RrgbValue%>,
			GrgbValue : <%=topColorsForIndustry[m].GrgbValue%>,
			BrgbValue : <%=topColorsForIndustry[m].BrgbValue%>
		})



	<%}%>
	/*****
		Logo Cloud data input
	******/
	var industryLength = <%= industryResult.length %>
	var indObjArray = Array();

	<% for(var i=0; i < industryResult.length; i++){ %>
	    var colorArrayObj = Array();
	    <% for(var j=0; j < industryResult[i].associatedColors.length; j++ ){%>

	        var colorIndObj = {
	            colorName : "<%=industryResult[i].associatedColors[j].colorName%>",
	            shade : "<%=industryResult[i].associatedColors[j].shade%>",
	            colorFamily : "<%=industryResult[i].associatedColors[j].colorFamily%>",
	            RrgbValue : <%=industryResult[i].associatedColors[j].RrgbValue%>,
	            GrgbValue : <%=industryResult[i].associatedColors[j].GrgbValue%>,
	            BrgbValue : <%=industryResult[i].associatedColors[j].BrgbValue%>,
	        }
	        colorArrayObj.push(colorIndObj);

	    <%}%>

		    var indObj = {
		        shortName : "<%= industryResult[i].shortName %>",
		        displayName : "<%= industryResult[i].displayName %>",
		        marketCap : "<%= industryResult[i].marketCap %>",
		        marketCapRank : "<%= industryResult[i].marketCapRank %>",
		        parentCompany : "<%= industryResult[i].parentCompany %>",
		        GICSIndName : "<%= industryResult[i].GICSIndName %>",
		        GICSSectorName : "<%= industryResult[i].GICSSectorName %>",
		        GICSSubIndName : "<%= industryResult[i].GICSSubIndName %>",
		        location : {
		            city : "<%= industryResult[i].city %>",
		            state : "<%= industryResult[i].state %>",
		            country : "<%= industryResult[i].country %>"
		        },
		        logoFileName : "<%= industryResult[i].logoFileName %>",
		        associatedColors : colorArrayObj
		    }

		    indObjArray.push(indObj);

		<%}%>
		
		<% 
		if(typeof brandResult !== 'undefined'){
			if(typeof brandResult.logoHistory !== 'undefined'){ %>
				brandHistoryResultLength = <%= brandResult.logoHistory.length %>
		<%}}%>
		<% if(queryType !== ''){ %>
			google.maps.event.addDomListener(window, 'load', initMaps);
		<%}%>
		
		</script>



<!-- Query Specific Javascript File-->
