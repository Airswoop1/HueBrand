<!--  Module - Company Brand Manual -->
    <div class="container-fluid" id="companybrandmanual">
      <div class="span12 well">
        <div class="title" >
          <button type="button" class="btn btn-default">      
            Company Brand Manual
          </button>
            <span class="pull-right muted">
              <div class="tooltips">
                <button type="button" class="btn btn-default" data-toggle="tooltip" data-placement="right" title="" data-original-title="Information on the Company Brand Manual Goes Here">
                  <i class="fa fa-info-circle"></i>
                </button>
              </div>
            </span>
        </div>
        <div class="well-content">
          <div id="companybrandmanual-box">
            <img src="../img/icons/general/manual.svg" style="height: 150px;float:left;margin-left:30px;">
            <i class="fa fa-long-arrow-right" style="float: left;font-size:100px;margin-left: 30px;margin-top: 29px;color: #000;"></i>
            <img src="../img/icons/general/pdf.svg" style="height:150px;float:left;margin-left:30px;">
          </div>
          <div style="width: 100%;text-align: center;">
          <a href="../Logos/<%= brandResult.brandManualFileName%>" download="<%= brandResult.brandManualFileName%>">
            <button id="buttondownload" style="padding-bottom: 10px;">
              <div>
                <i class="fa fa-download" style="font-size: 15px;margin-right: 10px;"></i>  
                Download <%= brandResult.shortName %>'s Original Brand Manual
              </div> 
            </button>
          </a>
          </div>
        </div>
      </div>
    </div>